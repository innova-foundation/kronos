//- script(src="/socket.io/socket.io.js")
script(src="https://cdnjs.cloudflare.com/ajax/libs/socket.io/2.3.0/socket.io.js")
script.
  var lanip = document.getElementById("lanip").getAttribute("data-value");
  var socket = io('//'+ lanip +':3300');
  socket.on('disconnect', () => {
      socket.open();
  });
  function togglesearch() {
      var x = document.getElementById("searchinput");
      if (x.style.display === "none") {
        x.style.display = "block";
      } else {
        x.style.display = "none";
      }
    }
  socket.on('notifications', function (data) {
      console.log('GOT NOTIFY', data);
      if (data.notifydb != '') {
        $('#notify').html("<div class='notifytx animate__animated animate__zoomInRight' id='notificationtx' style='background-color:#000000e8;border:3px solid #ffffff3d;border-radius:15px;padding:30px;font-size:13px;width:600px;position:fixed;right:0;bottom:5px;z-index:9999999;display:visible;' align='center'>Innova TX Notification<br/><a href='/tx/"+ data.notifydb.toString() +"' style='color:#999;font-size:0.85rem;'>"+ data.notifydb.toString() +"</a><div id='buttonfloat' style='top:36px;position:absolute;right:15px;' onclick='closeNotify()'><button type='submit' style='background-color:transparent;color:#666;border:none;' onclick='closeNotify()'>X</button></div></div>");
      }
  });
  function closeNotify() {
    var c = document.getElementById("notificationtx")
    c.style.display = "none";
  }
